CREATE TABLE TranscriptionEntity (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    timestamp TEXT NOT NULL,
    text TEXT NOT NULL
);

selectAll:
SELECT * FROM TranscriptionEntity ORDER BY id DESC;

insertItem:
INSERT INTO TranscriptionEntity (timestamp, text) VALUES (?, ?);

updateText:
UPDATE TranscriptionEntity SET text = ? WHERE id = ?;

deleteItem:
DELETE FROM TranscriptionEntity WHERE id = ?;